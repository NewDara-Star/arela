{
  "name": "@newdara/arela-setup",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "arela-setup": "dist/index.js"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx src/index.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepublishOnly": "pnpm run build",
    "postbuild": "chmod +x dist/index.js"
  },
  "dependencies": {
    "@newdara/preset-cto": "workspace:*",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "tsx": "^4.16.0",
    "typescript": "^5.5.4"
  }
}

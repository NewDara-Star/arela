{
  "name": "@newdara/preset-cto",
  "version": "1.3.0",
  "type": "module",
  "bin": {
    "arela": "dist/cli.js"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "templates",
    "scripts",
    "README.md",
    "CHANGELOG.md",
    "GETTING-STARTED.md",
    "QUICKSTART.md",
    "SETUP.md",
    "FLOW.md",
    "DEPENDENCIES.md",
    "RAG.md",
    "BROWSER-AUTOMATION.md",
    "AUTO-ACTIVATION.md",
    "FEATURES.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx src/cli.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "prepublishOnly": "pnpm run build",
    "postbuild": "chmod +x dist/cli.js",
    "postinstall": "node scripts/postinstall.cjs"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "execa": "^9.0.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.12",
    "gray-matter": "^4.0.3",
    "picocolors": "^1.0.0",
    "prompts": "^2.4.2",
    "semver": "^7.6.3",
    "yaml": "^2.5.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.7.4",
    "@types/prompts": "^2.4.9",
    "eslint": "^9.12.0",
    "prettier": "^3.3.3",
    "tsx": "^4.16.0",
    "typescript": "^5.5.4"
  }
}
